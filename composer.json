{
    "name": "dmoen/mailtrap-test-helper",
    "description": "Test helper libary for testing email submissions through Mailtrap, based on PHPUnit",
    "type": "libary",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Moen",
            "email": "moen.daniel@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "phpunit/phpunit": "^6.3"
    },
    "require-dev": {
        "phpmailer/phpmailer": "^6.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Dmoen\\MailtrapAssertions\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/helpers"]
    }
}
